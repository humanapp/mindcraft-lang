  "scripts": {
    "build": "npm run build:rbx && npm run build:node && npm run build:esm",
    "build:node": "tsc -p tsconfig.node.json && node scripts/post-build-node.js",
    "build:esm": "tsc -p tsconfig.esm.json && node scripts/post-build-esm.js",
    "build:rbx": "rbxtsc --type package --project tsconfig.rbx.json && node scripts/post-build-rbx.js",
    "watch:rbx": "node scripts/watch-rbx.js",
    "clean": "rm -rf ./dist",
    "lint": "biome lint ./src",
    "lint:fix": "biome lint ./src --write",
    "lint:eslint": "eslint \"src/**/*.ts\"",
    "format": "biome format ./src --write",
    "format:check": "biome format ./src",
    "check": "biome check ./src",
    "check:fix": "biome check ./src --write",
    "pretest": "npm run build:node",
    "test": "tsx --tsconfig tsconfig.node.json --test $(find src -name '*.spec.ts')",
    "prepublishOnly": "npm run build",
    "release:patch": "node scripts/release.js patch",
    "release:minor": "node scripts/release.js minor",
    "release:major": "node scripts/release.js major"
  },
