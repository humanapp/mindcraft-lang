{
  "name": "mindcraft-qwop",
  "description": "QWOP Clone -- brain-controlled ragdoll runner",
  "version": "0.1.0",
  "type": "module",
  "author": "humanapp",
  "license": "MIT",
  "scripts": {
    "dev": "vite --config vite/config.dev.mjs --force",
    "prebuild": "cd ../../packages/core && npm run build",
    "build": "vite build --config vite/config.prod.mjs",
    "clean": "rm -rf dist && rm -rf node_modules/.vite",
    "lint": "biome lint ./src",
    "lint:fix": "biome lint ./src --write",
    "format": "biome format ./src --write",
    "format:check": "biome format ./src",
    "check": "biome check ./src",
    "check:fix": "biome check ./src --write"
  },
  "dependencies": {
    "@mindcraft-lang/core": "file:../../packages/core",
    "@mindcraft-lang/ui": "file:../../packages/ui",
    "lucide-react": "^0.561.0",
    "phaser": "^3.90.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.15",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "terser": "^5.28.1",
    "typescript": "~5.7.2",
    "vite": "^6.3.1"
  }
}
