    "@mindcraft-lang/core": "file:../../packages/core",
    "@mindcraft-lang/ui": "file:../../packages/ui",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.561.0",
    "miniplex": "^2.0.0",
    "phaser": "^3.90.0",
    "react": "^19.0.0",
